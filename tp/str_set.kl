PROGRAM str_set
%NOBUSYLAMP
%NOLOCKGROUP

VAR
	out_reg : INTEGER
	str_val : STRING[32]
%from tpe.klh %import get_string_arg,get_int_arg
%from registers.klh %import set_string
%from systemlib.klh %import set_string

BEGIN
	str_val = tpe__get_string_arg(1)
	out_reg = tpe__get_int_arg(2)
	registers__set_string(out_reg, system__set_string(str_val))
END str_set