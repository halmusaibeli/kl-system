%ifndef source_h
%define source_h

%include namespace.m

%define prog_name system
%define prog_name_alias syslib

-- namespaced members in format <prog_name_alias>__<func_name>
declare(prog_name,date,prog_name_alias,date)
declare(prog_name,time,prog_name_alias,time)
declare(prog_name,millis,prog_name_alias,mills)

ROUTINE system__date : STRING FROM systemlib
ROUTINE system__time : STRING FROM systemlib
ROUTINE system__millis : INTEGER FROM systemlib

%endif --source_h